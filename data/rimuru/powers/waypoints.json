{
  "type": "origins:multiple",
  "bar": {
    "type": "origins:resource",
    "min": 0,
    "max": 3,
    "hud_render": {
      "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
      "bar_index": 13,
      "should_render": true,
      "condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:resource",
            "resource": "*:isekai_is_isekai",
            "comparison": "==",
            "compare_to": 1
          },
          {
            "type": "origins:resource",
            "resource": "*:switch_active_switch",
            "comparison": "==",
            "compare_to": 1100
          }
        ]
      }
    }
  },
  "if_set": {
    "type": "origins:resource",
    "min": 0,
    "max": 2,
    "hud_render": {
      "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
      "bar_index": 13,
      "should_render": true,
      "condition": {
        "type": "origins:resource",
        "resource": "*:switch_active_switch",
        "comparison": "==",
        "compare_to": 1101
      }
    }
  },
  "if_not_set": {
    "type": "origins:resource",
    "min": 0,
    "max": 1,
    "hud_render": {
      "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png",
      "bar_index": 13,
      "should_render": true,
      "condition": {
        "type": "origins:resource",
        "resource": "*:switch_active_switch",
        "comparison": "==",
        "compare_to": 1102
      }
    }
  },
  "check": {
    "type": "origins:resource",
    "min": 0,
    "max": 1,
    "hud_render": {
      "should_render": false
    }
  },
  "switch": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function rimuru:waypoints/switch"
    },
    "key": {
      "key": "key.origins.secondary_active"
    },
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:switch_active_switch",
          "comparison": ">=",
          "compare_to": 1100
        },
        {
          "type": "origins:resource",
          "resource": "*:switch_active_switch",
          "comparison": "<=",
          "compare_to": 1102
        }
      ]
    }
  },
  "action": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "function rimuru:waypoints/active"
    },
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type":"origins:and",
          "conditions": [
            {
              "type": "origins:resource",
              "resource": "*:switch_active_switch",
              "comparison": ">=",
              "compare_to": 1101
            },
            {
              "type": "origins:resource",
              "resource": "*:switch_active_switch",
              "comparison": "<=",
              "compare_to": 1102
            }
          ]
        },
        {
          "type": "origins:resource",
          "resource": "*:switch_active_switch",
          "comparison": "==",
          "compare_to": 1100
        },
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:command",
              "command": "execute if score @s rimuru_waypoints_use matches 0",
              "compare_to": 1,
              "comparison": "=="
            },
            {
              "type": "origins:resource",
              "resource": "*:switch_active_switch",
              "comparison": ">=",
              "compare_to": 1101
            },
            {
              "type": "origins:resource",
              "resource": "*:switch_active_switch",
              "comparison": "<=",
              "compare_to": 1102
            }
          ]
        },
        {
          "type":"origins:and",
          "conditions": [
            {
              "type": "origins:command",
              "command": "execute if score @s rimuru_switch_selected = @s rimuru_skill_waypoints",
              "compare_to": 1,
              "comparison": "=="
            },
            {
              "type": "origins:resource",
              "resource": "*:isekai_is_isekai",
              "comparison": "==",
              "compare_to": 1
            },
            {
              "type": "origins:resource",
              "resource": "*:switch_active_switch",
              "comparison": "==",
              "compare_to": 0
            }
          ]
        }
      ]
    }
  },
  "tp": {
    "type": "origins:action_over_time",
    "interval": 1,
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "/title @s actionbar \"§k wadawhd8awdha §r Teleporting, DO NOT MOVE §k wadawhd8awdha §\""
        },
        {
          "type":"origins:delay",
          "ticks": 200,
          "action": {
            "type":"origins:if_else",
            "condition":{
              "type": "origins:resource",
              "resource": "*:*_check",
              "comparison": "==",
              "compare_to": 1
            },
            "if_action": {
              "type":"origins:and",
              "actions":[
                {
                  "type": "origins:execute_command",
                  "command": "data modify storage rimuru:temp waypoint_status set value \"tp\""
                },
                {
                  "type": "origins:execute_command",
                  "command": "function rimuru:waypoints/tp3"
                }
              ]
            }
          }
        }
      ]
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_check",
      "comparison": "==",
      "compare_to": 1
    }
  },
  "tpreset": {
    "type": "origins:action_over_time",
    "interval": 1,
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "/title @s actionbar \"Teleporting canceled\""
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_check",
          "change": -1
        }
      ]
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:on_block",
          "inverted": true
        },
        {
          "type": "origins:moving"
        }
      ]
    }
  },
  "hidden": true
}